  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch People API",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/api/people-api/main.go",
      "cwd": "${workspaceFolder}/api/people-api",
      "showLog": true,
      "args": []
    },
    {
      "name": "Launch People GRPC",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/grpc/people-grpc/main.go",
      "cwd": "${workspaceFolder}/grpc/people-grpc",
      "args": ["-config-file=config.yml", "-port=2223"],
      "showLog": true
    }
  ]
}
